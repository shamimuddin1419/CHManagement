<style>
    .ml-1, .mx-1{
        margin-left:0rem !important;
    }
</style>
<div class="billGenerateForm">
    <div class="row">
        <div class="col-md-6">
            <div class="form-group requiredFormGroup">
                <label for="requestMonthId">Effective Month</label>
                <select class="form-control select2 border-danger" id="monthId">
                    <option value="0">Select Month</option>
                    <option value="1">January</option>
                    <option value="2">February</option>
                    <option value="3">March</option>
                    <option value="4">April</option>
                    <option value="5">May</option>
                    <option value="6">June</option>
                    <option value="7">July</option>
                    <option value="8">August</option>
                    <option value="9">September</option>
                    <option value="10">October</option>
                    <option value="11">November</option>
                    <option value="12">December</option>
                </select>
            </div>
        </div>

        <div class="col-md-6">
            <div class="form-group requiredFormGroup">
                <label for="requestYearId ">Effective Year</label>
                <select class="form-control select2 border-danger" id="yearId">
                    <option value="0">Select Year</option>
                </select>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            <div class="form-group">
                <label for="remarksId">Remarks</label>
                <input class="form-control" type="text" placeholder="Enter Remarks" id="remarksId" />
            </div>
        </div>


    </div>

    @*Commented to close individual bill generate*@
    <div class="row">
        <div class="col-md-6">
            <div class="form-group">
                <label for="isBatch" class="font-medium-2 text-bold-600 ml-1">Is Batch</label>&nbsp;
                <input type="checkbox" id="isBatch" class="switchery switchery-small" checked />
            </div>
        </div>
    </div>
    <div class="row individualCustomerDiv">
        <div class="col-md-6">
            <div class="form-group requiredFormGroup">
                <label for="projectId">Project</label>
                <select class="form-control  border-danger" id="projectId">
                    <option value="0">Select Project</option>
                </select>
            </div>
        </div>
        <div class="col-md-6">
            <div class="form-group">
                <label for="houseId">House</label>
                <select class="form-control select2 border-danger" id="houseId">
                    <option value="0">Select House</option>
                </select>
            </div>
        </div>
        <input type="hidden" id="renterHouseId" />
    </div>
</div>

